<idea-plugin version="2">
    <id>com.touwolf.idea.plugin.depschecker</id>
    <name>Java dependency checker</name>
    <version>0.1.0</version>
    <vendor email="it@touwolf.com"
            url="https://touwolf.com/">Touwolf
    </vendor>

    <description><![CDATA[
      Checks <strong>Java</strong> projects dependencies for version upgrades.
    ]]></description>

    <change-notes><![CDATA[
      Initial version.
    ]]>
    </change-notes>

    <idea-version since-build="145.0"/>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <action id="DepsChecker.CheckVersionAction" class="com.touwolf.idea.plugin.depschecker.CheckVersionAction"
                text="Check _Version" description="Checks dependencies versions with repository last version.">
            <add-to-group group-id="AnalyzeMenu" anchor="last"/>
        </action>
    </actions>
</idea-plugin>
